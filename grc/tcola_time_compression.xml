<block>
  <name>Time compression</name>
  <key>tcola_time_compression</key>
  <category>TCOLA</category>
  <import>import tcola</import>
  <make>tcola.time_compression_${type}($windowSize, $hopSize, $window)</make>
  <param>
    <name>Type</name>
    <key>type</key>
    <value>f</value>
    <type>enum</type>
    <option>
        <name>Float</name>
        <key>f</key>
        <opt>stream:float</opt>
    </option>
    <option>
        <name>Complex</name>
        <key>c</key>
        <opt>stream:complex</opt>
    </option>
  </param>
  <param>
    <name>Window Size</name>
    <key>windowSize</key>
    <type>int</type>
  </param>
  <param>
    <name>Hop Size</name>
    <key>hopSize</key>
    <type>int</type>
  </param>
  <param>
    <name>Window</name>
    <key>window</key>
    <type>real_vector</type>
  </param>
  <sink>
    <name>in</name>
    <type>$type.stream</type>
  </sink>
  <source>
    <name>out</name>
    <type>$type.stream</type>
  </source>
</block>
